<?xml version="1.0" encoding="UTF-8"?>
<!-- 복잡한 시골 마을 도로 - 노드 정의 -->
<!-- 마을 중심 + 순환도로 + 갈림길 + 막다른 길 -->
<nodes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/nodes_file.xsd">

    <!-- ========== 메인 도로 (동서 관통) ========== -->
    <node id="main_west" x="0.0" y="500.0" type="priority"/>
    <node id="village_entrance" x="400.0" y="500.0" type="priority"/>
    <node id="village_center" x="800.0" y="500.0" type="traffic_light"/>  <!-- 마을 중심 사거리 -->
    <node id="village_exit" x="1200.0" y="500.0" type="priority"/>
    <node id="main_east" x="1600.0" y="500.0" type="priority"/>

    <!-- ========== 마을 순환도로 ========== -->
    <!-- 북쪽 순환 -->
    <node id="north_curve1" x="600.0" y="700.0" type="priority"/>
    <node id="north_top" x="800.0" y="800.0" type="priority"/>
    <node id="north_curve2" x="1000.0" y="700.0" type="priority"/>

    <!-- 남쪽 순환 -->
    <node id="south_curve1" x="600.0" y="300.0" type="priority"/>
    <node id="south_bottom" x="800.0" y="200.0" type="priority"/>
    <node id="south_curve2" x="1000.0" y="300.0" type="priority"/>

    <!-- ========== 북쪽 지선 (교회, 학교) ========== -->
    <node id="church_junction" x="800.0" y="900.0" type="priority"/>
    <node id="church" x="700.0" y="1000.0" type="priority"/>
    <node id="school" x="900.0" y="1000.0" type="priority"/>

    <!-- ========== 남쪽 지선 (논밭) ========== -->
    <node id="farm_junction" x="800.0" y="100.0" type="priority"/>
    <node id="farm_west" x="600.0" y="50.0" type="priority"/>
    <node id="farm_east" x="1000.0" y="50.0" type="priority"/>

    <!-- ========== 동쪽 Y자 갈림길 ========== -->
    <node id="east_fork" x="1400.0" y="500.0" type="priority"/>
    <node id="mountain_road" x="1500.0" y="650.0" type="priority"/>
    <node id="riverside_road" x="1500.0" y="350.0" type="priority"/>

    <!-- ========== 서쪽 주택가 골목 ========== -->
    <node id="west_alley1" x="300.0" y="550.0" type="priority"/>
    <node id="west_alley2" x="300.0" y="450.0" type="priority"/>
    <node id="house_cluster" x="200.0" y="500.0" type="priority"/>

    <!-- ========== 마을회관/슈퍼 ========== -->
    <node id="community_center" x="850.0" y="600.0" type="priority"/>
    <node id="supermarket" x="750.0" y="400.0" type="priority"/>

</nodes>
